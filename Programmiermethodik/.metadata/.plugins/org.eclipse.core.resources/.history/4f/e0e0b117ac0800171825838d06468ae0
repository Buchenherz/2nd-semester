public class MyFirstProgram {
	
	// Is executed when programm is started
	public 	static void main(String[] args) {
		
		// Prints Hello World to the console
		System.out.println("Hello World");

		// Integer variables x, y, and z get declared & assigned
		int x, y, z;
		x = 2;
		y = 3;
		z = x * y;
		
		// The result of z (= x * z) is printed out to the console
		System.out.println("Result: x*y = " + z + " where x: " + x + 
		" and y: " + y);
	}
}